{
  "name": "wikipedia-analytics-client",
  "type": "module",
  "private": true,
  "scripts": {
    "prebuild:tailwind": "mkdir -p ./dist/styles",
    "prebuild:js": "mkdir -p ./dist/js",
    "build:tailwind": "npx @tailwindcss/cli -i ./styles/tailwind-input.css -o ./dist/styles/tailwind-output.css ",
    "build:tailwind:dev": "npx @tailwindcss/cli -i ./styles/tailwind-input.css -o ./dist/styles/tailwind-output.css --watch",
    "build:css": "build:tailwind && cp ./styles/global.css ./dist/styles/global.css",
    "build:js": "tsup",
    "build:js:dev": "tsup --watch",
    "typecheck": "tsc"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "chart.js": "^4.4.9"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.4",
    "@types/bun": "latest",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4",
    "tsup": "^8.4.0"
  }
}